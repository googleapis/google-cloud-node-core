{
  "name": "gaxios",
  "version": "1.0.3",
  "description": "A simple common HTTP client specifically for Google APIs and services.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "scripts": {
    "lint": "semistandard && npm run check",
    "test": "nyc mocha build/test",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "clean": "gts clean",
    "compile": "tsc -p . && npm run fix",
    "fix": "gts fix && semistandard --fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "coverage": "nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "repository": "JustinBeckwith/gaxios",
  "keywords": [
    "google"
  ],
  "author": "Google, LLC",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/extend": "^3.0.0",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.5.6",
    "@types/node-fetch": "^2.1.2",
    "@types/sinon": "^5.0.7",
    "assert-rejects": "^1.0.0",
    "codecov": "^3.0.4",
    "gts": "^0.9.0",
    "mocha": "^5.2.0",
    "nock": "^9.6.0",
    "nyc": "^12.0.2",
    "semistandard": "^13.0.1",
    "sinon": "^7.1.1",
    "source-map-support": "^0.5.6",
    "typescript": "~3.2.0"
  },
  "dependencies": {
    "extend": "^3.0.2",
    "https-proxy-agent": "^2.2.1",
    "node-fetch": "^2.2.0"
  },
  "nyc": {
    "exclude": [
      "build/test"
    ]
  }
}
